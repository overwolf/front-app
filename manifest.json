{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "Front App",
    "description": "",
    "version": "0.0.0",
    "minimum-overwolf-version": "0.165.0",
    "author": "Overwolf",
    "icon": "assets-static/dock.png",
    "icon_gray": "assets-static/dock-gray.png",
    "launcher_icon": "assets-static/launcher.ico",
    "window_icon": "assets-static/dock.png"
  },
  "permissions": [
    "Extensions",
    "Streaming",
    "GameInfo",
    "Profile",
    "Hotkeys",
    "DesktopStreaming",
    "FileSystem",
    "Clipboard"
  ],
  "data": {
    "force_browser": "user",
    "start_window": "background",
    "protocol_override_domains": {
      "googlesyndication": "http"
    },
    "hotkeys": {
      "toggle-front-app": {
        "title": "Show/Hide Front App",
        "action-type": "toggle",
        "default": "Ctrl+T"
      }
    },
    "windows": {
      "background": {
        "file": "background.html",
        "is_background_page": true,
        "background_optimization": false
      },
      "desktop": {
        "file": "desktop.html",
        "transparent": true,
        "resizable": false,
        "show_in_taskbar": true,
        "desktop_only": true,
        "native_window": true,
        "keep_window_location": true,
        "override_on_update": true,
        "block_top_window_navigation": true,
        "popup_blocker": true,
        "mute": true,
        "size": {
          "width": 1212,
          "height": 660
        },
        "start_position": {
          "top": 200,
          "left": 200
        }
      },
      "loading": {
        "file": "loading.html",
        "transparent": true,
        "resizable": false,
        "in_game_only": true,
        "show_in_taskbar": false,
        "keep_window_location": true,
        "override_on_update": true,
        "block_top_window_navigation": true,
        "popup_blocker": true,
        "mute": true,
        "size": {
          "width": 480,
          "height": 600
        },
        "start_position": {
          "top": 400,
          "left": 200
        }
      },
      "ingame": {
        "file": "ingame.html",
        "transparent": true,
        "resizable": false,
        "in_game_only": true,
        "show_in_taskbar": false,
        "keep_window_location": true,
        "override_on_update": true,
        "block_top_window_navigation": true,
        "popup_blocker": true,
        "mute": true,
        "focus_game_takeover": "ReleaseOnLostFocus",
        "size": {
          "width": 1212,
          "height": 660
        },
        "start_position": {
          "top": 200,
          "left": 200
        }
      },
      "notices": {
        "file": "notices.html",
        "transparent": true,
        "resizable": false,
        "topmost": true,
        "show_in_taskbar": false,
        "grab_focus_on_desktop": false,
        "keep_window_location": true,
        "override_on_update": true,
        "size": {
          "width": 500,
          "height": 800
        },
        "start_position": {
          "top": 10,
          "left": 10
        }
      }
